{
    "version": 2,
    "name": "client",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
      {
        "source": "/static/**/*.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      },
      {
        "source": "/static/**/*.mjs",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/ecmascript"
          }
        ]
      }
    ]
  }